{"remainingRequest":"C:\\project\\e-commerce\\front\\node_modules\\babel-loader\\lib\\index.js!C:\\project\\e-commerce\\front\\src\\utils\\request.js","dependencies":[{"path":"C:\\project\\e-commerce\\front\\src\\utils\\request.js","mtime":1667460378946},{"path":"C:\\project\\e-commerce\\front\\babel.config.js","mtime":1621952492000},{"path":"C:\\project\\e-commerce\\front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\project\\e-commerce\\front\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["axios","router","Vue","BASE_URL","API","create","baseURL","timeout","interceptors","request","use","config","headers","user","sessionStorage","getItem","JSON","parse","token","error","Promise","reject","response","res","data","responseType","code","removeItem","push","path","console","log"],"sources":["C:/project/e-commerce/front/src/utils/request.js"],"sourcesContent":["import axios from 'axios'\r\nimport router from '../router'\r\nimport Vue from 'vue'\r\n\r\n// 定义后台访问路径\r\nconst BASE_URL = \"http://zhinengwangluo.natapp1.cc\"\r\n\r\n// create an axios instance\r\nconst API = axios.create({\r\n    baseURL: BASE_URL, // url = base url\r\n    timeout: 60000 // request timeout\r\n});\r\n\r\n// request 拦截器\r\n// 可以自请求发送前对请求做一些处理\r\n// 比如统一加token，对请求参数统一加密\r\nAPI.interceptors.request.use(config => {\r\n    config.headers['Content-Type'] = 'application/json;charset=utf-8';\r\n    let user = sessionStorage.getItem(\"user\") || '{}';\r\n    user = JSON.parse(user);\r\n    config.headers['token'] = user.token;\r\n    return config\r\n}, error => {\r\n    return Promise.reject(error)\r\n});\r\n\r\n// response 拦截器\r\n// 可以在接口响应后统一处理结果\r\nAPI.interceptors.response.use(\r\n    response => {\r\n        let res = response.data;\r\n        // 如果是返回的文件\r\n        if (response.config.responseType === 'blob') {\r\n            return res\r\n        }\r\n        // 兼容服务端返回的字符串数据\r\n        if (typeof res === 'string') {\r\n            res = res ? JSON.parse(res) : res\r\n        }\r\n        if (res.code === '401') {\r\n            // 清除缓存\r\n            sessionStorage.removeItem(\"user\");\r\n            router.push({path: '/login'});\r\n            return res;\r\n        }\r\n        return res;\r\n    },\r\n    error => {\r\n        console.log('err' + error) // for debug\r\n        return Promise.reject(error)\r\n    }\r\n)\r\n\r\n\r\nexport default API\r\n"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,OAAOC,MAAM,MAAM,WAAW;AAC9B,OAAOC,GAAG,MAAM,KAAK;;AAErB;AACA,MAAMC,QAAQ,GAAG,kCAAkC;;AAEnD;AACA,MAAMC,GAAG,GAAGJ,KAAK,CAACK,MAAM,CAAC;EACrBC,OAAO,EAAEH,QAAQ;EAAE;EACnBI,OAAO,EAAE,KAAK,CAAC;AACnB,CAAC,CAAC;;AAEF;AACA;AACA;AACAH,GAAG,CAACI,YAAY,CAACC,OAAO,CAACC,GAAG,CAACC,MAAM,IAAI;EACnCA,MAAM,CAACC,OAAO,CAAC,cAAc,CAAC,GAAG,gCAAgC;EACjE,IAAIC,IAAI,GAAGC,cAAc,CAACC,OAAO,CAAC,MAAM,CAAC,IAAI,IAAI;EACjDF,IAAI,GAAGG,IAAI,CAACC,KAAK,CAACJ,IAAI,CAAC;EACvBF,MAAM,CAACC,OAAO,CAAC,OAAO,CAAC,GAAGC,IAAI,CAACK,KAAK;EACpC,OAAOP,MAAM;AACjB,CAAC,EAAEQ,KAAK,IAAI;EACR,OAAOC,OAAO,CAACC,MAAM,CAACF,KAAK,CAAC;AAChC,CAAC,CAAC;;AAEF;AACA;AACAf,GAAG,CAACI,YAAY,CAACc,QAAQ,CAACZ,GAAG,CACzBY,QAAQ,IAAI;EACR,IAAIC,GAAG,GAAGD,QAAQ,CAACE,IAAI;EACvB;EACA,IAAIF,QAAQ,CAACX,MAAM,CAACc,YAAY,KAAK,MAAM,EAAE;IACzC,OAAOF,GAAG;EACd;EACA;EACA,IAAI,OAAOA,GAAG,KAAK,QAAQ,EAAE;IACzBA,GAAG,GAAGA,GAAG,GAAGP,IAAI,CAACC,KAAK,CAACM,GAAG,CAAC,GAAGA,GAAG;EACrC;EACA,IAAIA,GAAG,CAACG,IAAI,KAAK,KAAK,EAAE;IACpB;IACAZ,cAAc,CAACa,UAAU,CAAC,MAAM,CAAC;IACjC1B,MAAM,CAAC2B,IAAI,CAAC;MAACC,IAAI,EAAE;IAAQ,CAAC,CAAC;IAC7B,OAAON,GAAG;EACd;EACA,OAAOA,GAAG;AACd,CAAC,EACDJ,KAAK,IAAI;EACLW,OAAO,CAACC,GAAG,CAAC,KAAK,GAAGZ,KAAK,CAAC,EAAC;EAC3B,OAAOC,OAAO,CAACC,MAAM,CAACF,KAAK,CAAC;AAChC,CAAC,CACJ;AAGD,eAAef,GAAG"}]}